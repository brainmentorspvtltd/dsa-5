class Solution {
    public int findTheWinner(int n, int k) {
        
        
        Queue<Integer> q=new LinkedList<Integer>();
        
        for(int i=1;i<=n;i++){
            q.add(i);
        }
        
        int m=1;
        while(q.size()!=1){
            if(m==k){
                q.remove();
                m=1;

            }
            else{
                q.add(q.remove());
                m++;
            }
        }
      
        return q.peek();
        
        
    }
}
